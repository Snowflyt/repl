{
  "name": "js-repl",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "postinstall": "patch-package",
    "dev": "vite",
    "lint": "eslint **/*.{js,jsx,ts,tsx} *.{cjs,mjs,cts,mts} --no-error-on-unmatched-pattern --report-unused-disable-directives-severity error --max-warnings 0",
    "lint:fix": "eslint --fix **/*.{js,jsx,ts,tsx} *.{cjs,mjs,cts,mts} --no-error-on-unmatched-pattern --report-unused-disable-directives-severity error --max-warnings 0",
    "prepare": "node -e \"import fs from 'fs'; import path from 'path'; const hooksDir = path.join(process.cwd(), '.githooks'); const gitHooksDir = path.join(process.cwd(), '.git/hooks'); if (!fs.existsSync(gitHooksDir)) { console.error('Git hooks directory not found, please run this in a git repository.'); process.exit(1); } fs.readdirSync(hooksDir).forEach(file => { const srcFile = path.join(hooksDir, file); const destFile = path.join(gitHooksDir, file); fs.copyFileSync(srcFile, destFile); if (process.platform !== 'win32' && !file.endsWith('.cmd')) { fs.chmodSync(destFile, 0o755); } })\"",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@typescript/ata": "^0.9.8",
    "@typescript/vfs": "^1.6.2",
    "ansi_up": "^6.0.6",
    "clsx": "^2.1.1",
    "color2k": "^2.0.3",
    "dom-to-image-more": "^3.7.2",
    "effect": "^3.18.5",
    "highlight.js": "^11.11.1",
    "html-minifier-terser": "^7.2.0",
    "js-untar": "2.0.0",
    "lz-string": "^1.5.0",
    "magic-bytes.js": "^1.12.1",
    "marked": "^17.0.0",
    "marked-highlight": "^2.2.3",
    "megamatch": "^0.1.9",
    "pako": "^2.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "semver": "^7.7.3",
    "showify": "^0.2.6",
    "troza": "^0.2.1",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@eslint/js": "^9.39.1",
    "@iconify/react": "^6.0.2",
    "@tailwindcss/vite": "^4.1.17",
    "@types/dom-to-image": "^2.6.7",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/pako": "^2.0.4",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/semver": "^7.7.1",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^61.3.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "globals": "^16.5.0",
    "patch-package": "^8.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.17",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.2"
  }
}
